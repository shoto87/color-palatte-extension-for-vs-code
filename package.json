{
  "name": "palette",
  "displayName": "color palette",
  "description": "generate palettes using single shades",
  "version": "0.1.2",
  "publisher": "pratikkodape",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoto87/color-palatte-extension-for-vs-code"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generatePalette",
        "title": "Generate Color Palette"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "publish": "npm version patch && vsce publish"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.4",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "chroma-js": "^3.1.2"
  }
}
